    "@types/lodash.uniqueid": "^4.0.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/tinycolor2": "^1.4.3",
    "babel-jest": "^26.6.0",
